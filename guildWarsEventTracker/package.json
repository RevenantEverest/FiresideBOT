{
  "name": "fireside-guildwars_event_tracker",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "yarn build && NODE_ENV=development node --inspect dist/server.js",
    "dev": "nodemon -L --ext ts --exec \"yarn build-dev\"",
    "start": "yarn build && NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand",
    "reset:npm": "rm -rf node_modules && yarn install"
  },
  "type": "module",
  "author": "RevenantEverest",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "chalk": "^5.2.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@swc/jest": "^0.2.24",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}

{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
        "**/__tests__/**/*.test.ts",
        "**/__tests__/**/*.spec.ts"
    ],
    "transform": {
        "^.+\\.(t|j)sx?$": [
            "@swc/jest"
        ]
    },
    "testSequencer": "./jestTestSequencer.cjs",
    "resolver": "ts-jest-resolver",
    "verbose": true,
    "forceExit": true,
    "clearMocks": true,
    "resetMocks": true,
    "restoreMocks": true
}
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.14",
    "@types/supertest": "^2.0.12",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"

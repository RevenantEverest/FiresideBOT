    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "play-dl": "^1.9.6",
    "reflect-metadata": "^0.1.13",
    "topgg-autoposter": "^2.0.1",
    "typeorm": "^0.3.9",
    "validate.js": "^0.13.1"
  },

{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "baseUrl": "./src",
        "paths": {
            "@@root/*": ["./*"],
            "@@api/*": ["./api/*"],
            "@@constants/*": ["./constants/*"],
            "@@commands/*": ["./commands/*"],
            "@@config/*": ["./config/*"],
            "@@controllers/*": ["./controllers/*"],
            "@@middleware/*": ["./middleware/*"],
            "@@resources/*": ["./resources/*"],
            "@@services/*": ["./services/*"],
            "@@types/*": ["./types/*"],
            "@@utils/*": ["./utils/*"]
        },
        "parser": {
            "syntax": "typescript",
            "decorators": true
        },
        "target": "es2022"
    }
}
{
  "name": "fireside-discord",
  "version": "2.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "bun run build && NODE_ENV=development node --inspect dist/server.js",
    "dev": "nodemon -L --ext ts --exec \"bun run build-dev\"",
    "start": "bun run build && NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test && jest --setupFiles dotenv/config --runInBand",
    "reset:npm": "rm -rf node_modules && bun install"
  },
  "type": "module",
  "author": "RevenantEverest",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.0.1",
    "@discordjs/voice": "^0.16.0",
    "@ybd-project/ytdl-core": "^6.0.8",
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "discord-api-types": "^0.37.56",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "opusscript": "^0.1.1",
    "sodium-native": "^4.0.4",
    "validate.js": "^0.13.1",
    "yt-search": "^2.12.1"
  },

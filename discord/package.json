    "@types/jest": "^28.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }

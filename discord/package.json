{
  "name": "fireside-discord",
  "version": "2.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "yarn build && NODE_ENV=development node --inspect dist/server.js",
    "dev": "nodemon -L --ext ts --exec \"yarn build-dev\"",
    "start": "yarn build && NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand",
    "reset:npm": "rm -rf node_modules && yarn install"
  },
  "type": "module",
  "author": "RevenantEverest",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.10.0",
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "libsodium-wrappers": "^0.7.10",
    "morgan": "^1.10.0",
    "validate.js": "^0.13.1",
    "ytdl-core": "^4.11.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
